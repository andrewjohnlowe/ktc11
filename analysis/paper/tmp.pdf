      _paq.push(['trackSiteSearch', querySubject, 'Archive SubjectFacet Search', false]);
    }

    if (queryWhere) {
      _paq.push(['setCustomVariable','1', queryWhere, 'ArchiveWhereFacetSearch']);
      _paq.push(['trackSiteSearch', queryWhere, 'Archive WhereFacet Search', false]);
    }

    if (login) { 
        _paq.push(['setCustomVariable','1', visitUrl, 'RegUser']);
    } else {
        _paq.push(['setCustomVariable','1', visitUrl, 'GuestUser']);
    }

    _paq.push(['setSiteId', 2]);
