  var _paq = _paq || [];
  _paq.push(['trackPageView']);
  _paq.push(['enableLinkTracking']);
  (function() {
    var u="//piwik.york.ac.uk/";
    _paq.push(['setTrackerUrl', u+'piwik.php']);

    if (keyQuery) {
      _paq.push(['setCustomVariable','1', keyQuery, 'ArchsearchKeywordSearch']);
      _paq.push(['trackSiteSearch', keyQuery, 'Archsearch Keyword Search', false]);
    }

    if (whereQuery) {
      _paq.push(['setCustomVariable','2', whereQuery, 'ArchsearchWhereFacetSearch']);
      _paq.push(['trackSiteSearch', whereQuery, 'Archsearch WhereFacet Search', false]);
